<h1>問題一覧</h1>
<% @questions.each do |question| %>
  <% if question.image.attached? %>
    <%= image_tag question.image, class:"d-block w-100" %>
    <% if question.user.present? %>
      <%= question.user.name %>
    <% end %>
    <% if admin_user_signed_in? %>
      <%=link_to "削除", admin_user_question_path(@admin_user, question), method: :delete %><br>
    <% end %>
   <% end %>
<% end %>
